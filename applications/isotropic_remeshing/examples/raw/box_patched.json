{
    "input": {
        "path": "cad/Box_patched.hdf5",
        "name_spec_file": "cad/Box_patched_names.json"
    },
    "position_attribute": "combo/vertex_position",
    "other_position_attributes": [
    ],
    "pass_through_attributes": [
        "combo.trims/edge_labels",
        "combo.patches/patch_labels"
    ],
    "static_mesh_names": [
        "combo.critical_points",
        "combo.trims"
    ],
    "iterations": 10,
    "length_rel": 0.1,
    "use_for_periodic": false,
    "edge_swap_mode": "valence",
    "use_split": true,
    "use_collapse": false,
    "use_swap": false,
    "use_smooth": true,
    "2envelope_size": 1e-3,
    "fix_uv_seam": false,
    "lock_boundary": false,
    "output": {
        "path": "box_tagged_output",
        "type": ".vtu"
    },
    "output2": {
        "path": "box_tagged_output.hdf5",
        "type": ".hdf5",
        "position_attribute": "vertex_position"
    },
    "intermediate_output_format2": {
        "pos": {
            "path": "box_{:04}",
            "type": ".vtu",
            "position_attribute": "vertex_position"
        }
    }
}
